    "format-icons": {
      "headphone": "ÔÄ•",
      "hands-free": "Ôñê",
      "headset": "Ôñê",
      "phone": "ÔÇï",
      "portable": "ÔÇï",
      "car": "ÔÜπ",
      "default": ["ÔÄ¶", "ÔÄß", "ÔÄ®"],
    },
    "on-click": "pavucontrol",
  },
  "custom/media": {
    "format": "{icon} {}",
    "return-type": "json",
    "format-icons": {
      "spotify": "ÔÜº ",
      "default": "Ô¢Ö ",
    },
    "escape": false,

