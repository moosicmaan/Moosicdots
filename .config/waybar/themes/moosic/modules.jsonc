    "tooltip": true,
    "tooltip-format": "System Status"
  },
  // UPDATES COUNT
  "custom/updates": {
    "format": "  {}",
    "escape": true,
    "return-type": "json",
    "exec-if": "which waybar-module-pacman-updates",
    "exec": "waybar-module-pacman-updates",
    // "exec": "~/.config/.scripts/updates.sh",
    "restart-interval": 60,
    "on-click": "kitty --class dotfiles-floating -e garuda-update",
    "on-click-right": "~/.config/.settings/software.sh"
  },
  "custom/updates#gar": {
    "format": "{} {icon}",
    "return-type": "json",
    "format-icons": {
      "has-updates": "󱍷",
      "updated": "󰂪",
      "interval": 7200
    },
    "exec-if": "which waybar-module-pacman-updates",
    "exec": "waybar-module-pacman-updates",
    "on-click": "kitty --class dotfiles-floating -e garuda-update"
  },
  // ---------------------------------------------------
  // AUDIO MODULES
  // ---------------------------------------------------
  // CAVA
  "cava": {
    //        "cava_config": "$XDG_CONFIG_HOME/cava/cava.conf",
    "framerate": 30,
    "autosens": 1,
    // "sensitivity": 100,
    "bars": 12,
    "lower_cutoff_freq": 10,
    "higher_cutoff_freq": 10000,
    "method": "pipewire",
    "source": "auto",
    "stereo": false,
    "reverse": false,
    "sleep_timer": 7,
    "hide_on_silence": true,
    // "bar_delimiter": "`",
    "bar_delimiter": 0,
    "monstercat": true,
    "waves": true,
    "noise_reduction": 0.57,
    "input_delay": 2,
    "format-icons": [
      "▁",
      "▂",
      "▃",
      "▄",
      "▅",
      "▆",
      "▇",
      "█"
    ],
    "actions": {
      "on-click-right": "mode"
    }
  },
  // PULSEAUDIO
  "pulseaudio": {
    // "scroll-step": 1, // %, can be a float
    "format": "{icon}  {volume}%",
    "format-bluetooth": " {volume}% {icon}   {format_source}",
    "format-bluetooth-muted": "  {icon}   {format_source}",
    "format-muted": " {format_source}",
    "format-source": "{volume}% ",
    "format-source-muted": "",
    "format-icons": {
      "headphone": "",
      "hands-free": "",
      "headset": "",
      "phone": "",
      "portable": "",
      "car": "",
      "default": [
        "",
        " ",
        " "
      ]
    },
    "on-click": "pavucontrol-qt"
  },
  // WIREPLUMBER
  "wireplumber": {
    "format": "{volume}% {icon}",
    "format-muted": "",
    "on-click": "helvum",
    "format-icons": [
      "",
      "",
      ""
    ]
  },
  // MPRIS
  "mpris": {
    "format": "{player_icon}{dynamic}",
    "format-paused": "{status_icon}<i>{dynamic}</i>",
    "max-length": 150,
    "player-icons": {
      "default": "▶ ",
      "mpv": "󰐹 ",
      // "spotube": "👠 ",
      "spotube": "󰎆 ",
      "spotify": " ",
      "chromium": " ",
      "firefox": " ",
      "firedragon": " ",
      "vlc": "󰕼 ",
      "kdeconnect": " ",
      "youtube": " "
    },
    "status-icons": {
      "paused": "󰐎 ",
      "playing": " ",
      "stopped": " "
    }
    // "ignored-players": ["firefox"]
  },
  //MPRIS COMPACT
  "mpris#comp": {
    "format": "{player_icon} ",
    "format-paused": "{status_icon} ",
    "max-length": 30,
    "tooltip": true,
    // "tooltip-format": "{source_desc}",
    "tooltip-format": "{status_icon} {dynamic}",
    "scroll-step": 5.0,
    "on-scroll-up": "pactl set-sink-volume @DEFAULT_SINK@ +2%",
    "on-scroll-down": "pactl set-sink-volume @DEFAULT_SINK@ -2%",
    "smooth-scrolling-threshold": 1,
    "player-icons": {
      "default": "▶ ",
      "chromium": "",
      "firefox": "",
      "firedragon": " ",
      "mpv": "󰐹",
      "spotify": "",
      // "spotube": "👠 ",
      "spotube": "󰎆 ",
      "vlc": "󰕼"
    },
    "status-icons": {
      "paused": "󰐎 ",
      "playing": " ",
      "stopped": " "
    }
    // "ignored-players": ["firefox"]
  },
  // ---------------------------------------------------
  // PROGRAMS
  // ---------------------------------------------------
  // SYSTEM TRAY
  "tray": {
    "icon-size": 14,
    "show-passive-items": true,
    "spacing": 5
  },
  // TASKBAR
  "wlr/taskbar": {
    "format": "{icon}",
    "icon-size": 14,
    "tooltip-format": "{title}",
    "on-click": "activate",
    "on-click-middle": "close",
    "ignore-list": [
      "Alacritty"
    ],
    "app_ids-mapping": {
      "firefoxdeveloperedition": "firefox-developer-edition"
    },
    "rewrite": {
      "Firefox Web Browser": "Firefox",
      "Foot Server": "Terminal"
    }
  },
  // ---------------------------------------------------
  // HARDWARE/SYSTEM MODULES
  // ---------------------------------------------------
  // USER
  "user": {
    "format": "{user} (up {work_d} days ↑)",
    "interval": 60,
    "height": 16,
    "width": 16,
    "icon": true
  },
  // HARDDISC SPACE USED
  "disk": {
    "interval": 30,
    "format": " D {percentage_used}% ",
    "path": "/",
    "on-click": "kitty -e btop"
  },
  // NETWORK
  "network": {
    "format": " {ifname}",
    "format-wifi": "    {signalStrength}%",
    "format-ethernet": "  {ifname}",
    "format-disconnected": " Disconnected",
    "tooltip-format": " {ifname} via {gwaddri}",
    "tooltip-format-wifi": "  {ifname} @ {essid}\nIP: {ipaddr}\nStrength: {signalStrength}%\nFreq: {frequency}MHz\nUp: {bandwidthUpBits} Down: {bandwidthDownBits}",
    "tooltip-format-ethernet": " {ifname}\nIP: {ipaddr}\n up: {bandwidthUpBits} down: {bandwidthDownBits}",
    "tooltip-format-disconnected": "Disconnected",
    "max-length": 50,
    "on-click": "~/.config/.settings/networkmanager.sh"
  },
  // Network Traffic
  "custom/network_traffic": {
    "exec": "~/.config/waybar/scripts/network_traffic.sh",
    "return-type": "json",
    "format-ethernet": "{icon} {ifname} ⇣{bandwidthDownBytes} ⇡{bandwidthUpBytes}"
    // optional
  },
  // IDLE INHIBATOR
  "idle_inhibitor": {
    "format": "{icon}",
    "format-icons": {
      "activated": " ",
      "deactivated": " "
    },
    "on-click-right": "swaylock"
  },
  // BATTERY
  "battery": {
    "states": {
      // "good": 95,
      "good": 95,
      "warning": 30,
      "critical": 15
    },
    "format": " {icon}  {capacity}% {time}",
    "format-charging": "   {capacity}% {time}",
    "format-plugged": "   {capacity}%",
    // "format-alt": " {icon}  {time}",
    // "format-good": "", // An empty format will hide the module
    "format-full": "",
    "format-icons": [
      " ",
      " ",
      " ",
      " ",
      " "
    ]
  },
  // KEYBOARD STATE
  "keyboard-state": {
    "numlock": true,
    "capslock": true,
    "format": " {name} {icon}",
    "format-icons": {
      "locked": " ",
      "unlocked": " "
    },
    "icon-size": 14
  },
  // BLUETOOTH
  "bluetooth": {
    "format": "  {status}",
    "format-connected": "  {device_alias}",
    "format-disabled": "",
    "format-off": "",
    "format-connected-battery": "  {device_alias} {device_battery_percentage}%",
    "interval": 30,
    "on-click": "blueman-manager",
    "format-no-controller": "",
    "tooltip": true,
    "tooltip-format": "{controller_alias}\t{controller_address}\n\n{num_connections} connected",
    "tooltip-format-connected": "{controller_alias}\t{controller_address}\n\n{num_connections} connected\n\n{device_enumerate}",
    "tooltip-format-enumerate-connected": "{device_alias}\t{device_address}",
    "tooltip-format-enumerate-connected-battery": "{device_alias}\t{device_address}\t{device_battery_percentage}%"
  },
  // CPU
  "cpu": {
    "format": "    {usage}% ",
    "on-click": "kitty -e btop"
  },
  // MEMORY
  "memory": {
    "format": " 🖴 {:>3}% ",
    "on-click": "kitty -e btop"
  },
  // TEMPERATURE
  "temperature": {
    "thermal-zone": 2,
    "hwmon-path": "/sys/class/hwmon/hwmon2/temp1_input",
    "critical-threshold": 80,
    "format-critical": "  {temperatureC}°C",
    "format": "  {temperatureC}°C"
  },
  // BACKLIGHT
  "backlight": {
    // "device": "acpi_video1",
    "format": " {icon} {percent}% ",
    "format-icons": [
      "",
      "",
      "",
      "",
      "",
      "",
      "",
      "",
      ""
    ],
    "on-scroll-down": "brightnessctl -c backlight set 1%-",
    "on-scroll-up": "brightnessctl -c backlight set +1%",
    "on-click": "~/.config/waybar/scripts/backlight-hint.sh"
  },
  // ---------------------------------------------------
  // LOCATION INFORMATION
  // ---------------------------------------------------
  // WEATHER APP DAVAO
  "custom/weatherD": {
    "format": "Davao:{}°",
    // "format": "{ICON}{temp_C}({FeelsLikeC})",
    "tooltip": true,
    "interval": 3600,
    // "exec": "wttrbar --date-format %m/%d --fahrenheit --mph --location 'Davao+City' --hide-conditions ",
    "exec": "wttrbar --date-format %m/%d  --fahrenheit --mph --location 'Davao+City'",
    "return-type": "json"
  },
  // WEATHER APP LOCAL
  "custom/weatherC": {
    "format": "Local {}°",
    // "format": "{ICON}{temp_C}({FeelsLikeC})",
    "tooltip": true,
    "interval": 3600,
    "exec": "wttrbar  --date-format %m/%d --fahrenheit --mph --location 'Walsenburg'",
    "return-type": "json"
  },
  // Weather
  // github.com/chubin/wttr.in
  "custom/weather#leg": {
    "exec": "curl 'https://wttr.in/Walsenburg?format=3'",
    "interval": 900,
    "on-click": "yad --html --uri='https://wttr.in/Walsenburg' --center --fixed --width=1000 --height=680 --timeout=60 --timeout-indicator=right"
  },
  // CLOCK
  "clock": {
    "format-alt": "{:%H:%M}  ",
    "format": "{:%a, %b %d, %Y -  %R  }",
    "tooltip-format": "<tt><small>{calendar}</small></tt>",
    "calendar": {
      "mode": "year",
      "mode-mon-col": 3,
      "weeks-pos": "right",
      "on-scroll": 1,
      "format": {
        // "months": "<span color='#ffead3'><b>{}</b></span>",
        "months": "<span color='#333333'><b>{}</b></span>",
        // "days": "<span color='#ecc6d9'><b>{}</b></span>",
        // "weeks": "<span color='#99ffdd'><b>W{}</b></span>",
        "weeks": "<span color='#9999dd'><b>W{}</b></span>",
        // "weekdays": "<span color='#ffcc66'><b>{}</b></span>",
        "today": "<span color='#ff6699'><b><u>{}</u></b></span>"
      }
    },
    "timezones": [
      "America/Denver",
      "Europe/London",
      "Asia/Manila"
    ],
    "actions": {
      "on-click-right": "mode",
      "on-click-forward": "tz_up",
      "on-click-backward": "tz_down",
      "on-scroll-up": "shift_up",
      "on-scroll-down": "shift_down"
    }
  },
  // CLOCK, DAVAO
  "clock#2": {
    "timezone": "Asia/Manila",
    "format": "{:%a- %R  }",
    "format-alt": "{:%H:%M}  ",
    "tooltip": true,
    "tooltip-format": "<tt><small>{calendar}</small></tt>",
    "actions": {
      "on-click-right": "mode"
    }
  },
  // Clock
  "clock#leg": {
    "tooltip-format": "<big>{:%A, %d.%B %Y }</big>\n<tt><small>{calendar}</small></tt>",
    "format": "|    {:%a %d %b -   %I:%M %p} |", //12 hour format
    //"format": " {:%a %d %b  %H:%M} ",	//24 hour format
    "format-alt": "  {:%d/%m/%Y -   %H:%M:%S} ",
    "timezones": [
      "America/Denver"
    ],
    "max-length": 200,
    "interval": 1,
    "on-click": "~/.config/waybar/scripts/OCV"
  },
  // Clock
  "clock#simple": {
    "timezone": "America/Denver",
    "format": "{:%H:%M}  ",
    "tooltip": true,
    "tooltip-format": "<tt><small>{calendar}</small></tt>",
    "actions": {
      "on-click-right": "mode"
    }
  },
