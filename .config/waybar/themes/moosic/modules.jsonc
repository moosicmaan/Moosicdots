    // "format": "🌐",
    // "format": " ",
    "format": "",
    // "on-click": "rofi -show drun -replace",
    "on-click": "exec nwg-drawer -c 7 -is 70 -spacing 23",
    // "on-click-right": "~/.config/.scripts/keybindings.sh",
    "on-click-right": "rofi -show drun -replace",
    "tooltip": false
  },
  // CUSTOM LAUNCHER
  "custom/launcher": {
    "format": "",
    //"format":"",
    //"format":"",
    "on-click": "exec nwg-drawer -c 7 -is 70 -spacing 23",
    "on-click-right": "~/.config/.scripts/keybindings.sh",
    "tooltip": false
  },
