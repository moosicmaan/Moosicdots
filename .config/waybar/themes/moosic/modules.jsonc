  "custom/weatherD": {
    "format": "Davao:{}°",
    "tooltip": true,
    "interval": 3600,
    "exec": "wttrbar --date-format %m/%d  --fahrenheit --mph --location 'Davao+City'",
    "return-type": "json"
  },
  "custom/weatherC": {
    "format": "Local {}°",
    "tooltip": true,
    "interval": 3600,
    "exec": "wttrbar  --date-format %m/%d --fahrenheit --mph --location 'Walsenburg'",
    "return-type": "json"
  },
