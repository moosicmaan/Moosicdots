  "tray": {
    "icon-size": 14,
    "show-passive-items": true,
    "spacing": 5
  },
  "wlr/taskbar": {
    "format": "{icon}",
    "icon-size": 14,
    "tooltip-format": "{title}",
    "on-click": "activate",
    "on-click-middle": "close",
    "ignore-list": [
      "Alacritty"
    ],
    "app_ids-mapping": {
      "firefoxdeveloperedition": "firefox-developer-edition"
    },
    "rewrite": {
      "Firefox Web Browser": "Firefox",
      "Foot Server": "Terminal"
    }
  },
  "user": {
    "format": "{user} (up {work_d} days ↑)",
    "interval": 60,
    "height": 16,
    "width": 16,
    "icon": true
  },
  "disk": {
    "interval": 30,
    "format": " D {percentage_used}% ",
    "path": "/",
    "on-click": "wezterm start -e btop"
  },
  "network": {
    "format": " {ifname}",
    "format-wifi": "    {signalStrength}%",
    "format-ethernet": " 󰈀 {ifname}",
    "format-disconnected": " Disconnected",
    "tooltip-format": "󰈀  {ifname} via {gwaddri}",
    "tooltip-format-wifi": "  {ifname} @ {essid}\nIP: {ipaddr}\nStrength: {signalStrength}%\nFreq: {frequency}MHz\nUp: {bandwidthUpBits} Down: {bandwidthDownBits}",
    "tooltip-format-ethernet": "󰈀  {ifname}\nIP: {ipaddr}\n up: {bandwidthUpBits} down: {bandwidthDownBits}",
    "tooltip-format-disconnected": "Disconnected",
    "max-length": 50,
    "on-click": "~/.config/.settings/networkmanager.sh"
  },
  "custom/network_traffic": {
    "exec": "~/.config/waybar/scripts/network_traffic.sh",
    "return-type": "json",
    "format-ethernet": "{icon} {ifname} ⇣{bandwidthDownBytes} ⇡{bandwidthUpBytes}"
  },
  "idle_inhibitor": {
    "format": "{icon}",
    "format-icons": {
      "activated": " ",
      "deactivated": " "
    },
    "on-click-right": "swaylock"
  },
  "battery": {
    "states": {
      "good": 95,
      "warning": 30,
      "critical": 15
    },
    "format": " {icon}  {capacity}% {time}",
    "format-charging": "   {capacity}% {time}",
    "format-plugged": "   {capacity}%",
    "format-full": "",
    "format-icons": [
      " ",
      " ",
      " ",
      " ",
      " "
    ]
  },
  "keyboard-state": {
    "numlock": true,
    "capslock": true,
    "format": " {name} {icon}",
    "format-icons": {
      "locked": " ",
      "unlocked": " "
    },
    "icon-size": 14
  },
  "bluetooth": {
    "format": "  {status}",
    "format-connected": "  {device_alias}",
    "format-disabled": "",
    "format-off": "",
    "format-connected-battery": "  {device_alias} {device_battery_percentage}%",
    "interval": 30,
    "on-click": "blueman-manager",
    "format-no-controller": "",
    "tooltip": true,
    "tooltip-format": "{controller_alias}\t{controller_address}\n\n{num_connections} connected",
    "tooltip-format-connected": "{controller_alias}\t{controller_address}\n\n{num_connections} connected\n\n{device_enumerate}",
