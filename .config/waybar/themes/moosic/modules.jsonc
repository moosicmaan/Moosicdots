  "cpu": {
    "format": "    {usage}% ",
    "on-click": "wezterm start -e btop"
  },
  "memory": {
    "format": " 🖴 {:>3}% ",
    "on-click": "wezterm start -e btop"
  },
  "temperature": {
    "thermal-zone": 2,
    "hwmon-path": "/sys/class/hwmon/hwmon2/temp1_input",
    "critical-threshold": 80,
    "format-critical": "  {temperatureC}°C",
    "format": "  {temperatureC}°C"
  },
  "backlight": {
    "format": " {icon} {percent}% ",
    "format-icons": [
      "",
      "",
      "",
      "",
      "",
      "",
      "",
      "",
      ""
