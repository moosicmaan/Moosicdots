    "exec": "curl 'https://wttr.in/Walsenburg?format=3'",
    "interval": 900,
    "on-click": "yad --html --uri='https://wttr.in/Walsenburg' --center --fixed --width=1000 --height=680 --timeout=60 --timeout-indicator=right"
  },
