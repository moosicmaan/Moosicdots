  // ---------------------------------------------------
  // WM-SPECIFIC MODULES
  // ---------------------------------------------------
  // HYPRLAND SUBMAP
  "hyprland/submap": {
    "format": "✌️ {}",
    "max-length": 18,
    "tooltip": false
  },
  // SWAY MODE
  "sway/mode": {
    "format": "<span style=\"italic\">{}</span>"
  },
  // SWAY WINDOW
  "sway/window": {
    //"all-outputs": true, // Uncomment for active window on all outputs
    "format": "{}",
    "rewrite": {
      "(.*) - Brave": "$1",
      "(.*) - Brave Search": "$1",
      "(.*) - Kitty": "$1"
    },
    "separate-outputs": true,
    "icon": true,
    "spacing": 5,
    "icon-size": 14
  },
  // HYPRLAND WINDOW
  "hyprland/window": {
    "rewrite": {
      "(.*) - Brave": "$1",
      "(.*) - Brave Search": "$1",
      "(.*) - Kitty": "$1"
    },
    "separate-outputs": true,
    "icon": true,
    "spacing": 5,
    "icon-size": 14
  },
  // DWL WINDOW
  "dwl/window": {
    "format": "{}",
    "icon": true,
    "icon-size": 14,
    "max-length": 50,
    "rewrite": {
      "(.*) - Mozilla Firefox": "🌎 $1",
      "(.*) - vim": " $1",
      "(.*) - zsh": " [$1]"
    }
  },
