    },
    "timezones": ["America/Denver", "Europe/London", "Asia/Manila"],
    "actions": {
      "on-click-right": "mode",
      "on-click-forward": "tz_up",
      "on-click-backward": "tz_down",
      "on-scroll-up": "shift_up",
