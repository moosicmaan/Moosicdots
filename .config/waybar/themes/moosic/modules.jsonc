  "cpu": {
    "format": "/ C {usage}% ",
    "on-click": "kitty -e btop",
  },

  // MEMORY
  "memory": {
    "format": "/ M {}% ",
    "on-click": "kitty -e btop",
  },

  // HARDDISC SPACE USED
  "disk": {
    "interval": 30,
    "format": "/ D {percentage_used}% ",
    "path": "/",
