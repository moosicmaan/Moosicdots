{
  "mpris": {
    "format": "{player_icon} {dynamic}",
    "format-paused": "{status_icon} <i>{dynamic}</i>",
    "max-length": 200,
    "player-icons": {
      "default": "‚ñ∂ ",
      "mpv": "üéµ",
      "spotube": "üë† "
    },
    "status-icons": {
      "paused": "‚è∏"
    }
  },
  "hyprland/workspaces": {
    "on-click": "activate",
    "active-only": false,
    "all-outputs": true,
    "format": "{}",
    "format-icons": {
      "urgent": "",
      "active": "",
      "default": ""
    },
    "persistent-workspaces": {
      "*": 4
    }
  },
  "hyprland/submap": {
    "format": "‚úåÔ∏è {}",
    "max-length": 18,
    "tooltip": false
  },
  "wlr/taskbar": {
    "format": "{icon}",
    "icon-size": 18,
    "tooltip-format": "{title}",
    "on-click": "activate",
    "on-click-middle": "close",
    "ignore-list": [
      "Alacritty"
    ],
    "app_ids-mapping": {
      "firefoxdeveloperedition": "firefox-developer-edition"
    },
    "rewrite": {
      "Firefox Web Browser": "Firefox",
      "Foot Server": "Terminal"
    }
  },
  "hyprland/window": {
    "rewrite": {
      "(.*) - Brave": "$1",
      "(.*) - Brave Search": "$1",
      "(.*) - Kitty": "$1",
      "(.*) - Wezterm": "$1"
    },
    "separate-outputs": true,
    "icon": true,
    "spacing": 5,
    "icon-size": 14
  },
  "custom/cliphist": {
    "format": "ÔÉ™",
    "on-click": "sleep 0.1 && ~/.config/.scripts/cliphist.sh",
    "on-click-right": "sleep 0.1 && ~/.config/.scripts/cliphist.sh d",
    "on-click-middle": "sleep 0.1 && ~/.config/.scripts/cliphist.sh w",
    "tooltip": false
  },
  "custom/updates": {
    "format": "ÔÇ´  {}",
    "tooltip-format": "{}",
    "escape": true,
    "return-type": "json",
    "exec": "~/.config/.scripts/updates.sh",
    "restart-interval": 60,
    "on-click": "wezterm start --class dotfiles-floating -e garuda-update",
    "on-click-right": "~/.config/.settings/software.sh",
    "tooltip": false
  },
  "custom/wallpaper": {
    "format": "ÔÄæ",
    "on-click": "~/.config/.scripts/wallpaper.sh select",
    "on-click-right": "~/.config/.scripts/wallpaper.sh",
    "tooltip": false
  },
  "custom/waybarthemes": {
    "format": "ÔÖÅ",
    "on-click": "~/.config/.scripts/themeswitcher.sh",
    "tooltip": false
  },
  "custom/settings": {
    "format": "ÔÄì",
    "on-click": "wezterm start --class dotfiles-floating -e ~/.config/.settings/start-settings.sh",
    "tooltip": false
  },
  "custom/keybindings": {
    "format": "ÔÑú",
    "on-click": "~/.config/.scripts/keybindings.sh",
    "tooltip": false
  },
  "custom/filemanager": {
    "format": "ÔÅº",
    "on-click": "~/.config/.settings/filemanager.sh",
    "tooltip": false
  },
  "custom/browser": {
    "format": "üñß",
    "on-click": "~/.config/.settings/browser.sh",
    "tooltip": false
  },
  "custom/chatgpt": {
    "format": " ",
    "on-click": "brave --app=https://chat.openai.com",
    "tooltip": false
  },
  "custom/calculator": {
    "format": "Ôá¨",
    "on-click": "qalculate-gtk",
    "tooltip": false
  },
  "custom/appmenu": {
    "format": "Apps",
    "on-click": "exec nwg-drawer -c 7 -is 70 -spacing 23",
    "on-click-right": "~/.config/.scripts/keybindings.sh",
    "tooltip": false
  },
  "custom/appmenuicon": {
    "format": "ÔåÉ",
    "on-click": "rofi -show drun -replace",
    "on-click-right": "~/.config/.scripts/keybindings.sh",
    "tooltip": false
  },
  "custom/exit": {
    "format": "ÔÄë",
    "on-click": "wlogout",
    "tooltip": false
  },
  "keyboard-state": {
    "numlock": true,
    "capslock": true,
    "format": "{name} {icon}",
    "format-icons": {
      "locked": "ÔÄ£",
      "unlocked": "ÔÇú"
    }
  },
  "tray": {
    "icon-size": 21,
    "spacing": 10
  },
  "clock": {
    "tooltip-format": "<big>{:%Y %B}</big>\n<tt><small>{calendar}</small></tt>",
    "format-alt": "{:%Y-%m-%d}"
  },
  "custom/system": {
    "format": "Ó´∏",
    "tooltip": false
  },
  "cpu": {
    "format": "/ C {usage}% ",
    "on-click": "wezterm start -e htop"
  },
  "memory": {
    "format": "/ M {}% ",
    "on-click": "wezterm start -e htop"
  },
  "disk": {
    "interval": 30,
    "format": "D {percentage_used}% ",
    "path": "/",
    "on-click": "wezterm start -e htop"
  },
  "hyprland/language": {
    "format": "/ K {short}"
  },
  "group/hardware": {
    "orientation": "inherit",
    "drawer": {
      "transition-duration": 300,
      "children-class": "not-memory",
      "transition-left-to-right": false
    },
    "modules": [
      "custom/system",
      "disk",
      "cpu",
      "memory",
      "hyprland/language"
    ]
  },
  "group/settings": {
    "orientation": "horizontal",
    "modules": [
      "custom/chatgpt",
      "custom/waybarthemes",
      "custom/wallpaper"
    ]
  },
  "group/quicklinks": {
    "orientation": "horizontal",
    "modules": [
      "custom/browser",
      "custom/filemanager"
    ]
  },
  "network": {
    "format": "{ifname}",
    "format-wifi": "Ôá´   {signalStrength}%",
    "format-ethernet": "Û∞àÄ {ifname}",
    "format-disconnected": "Disconnected",
    "tooltip-format": "Û∞àÄ {ifname} via {gwaddri}",
    "tooltip-format-wifi": "Ôá´  {ifname} @ {essid}\nIP: {ipaddr}\nStrength: {signalStrength}%\nFreq: {frequency}MHz\nUp: {bandwidthUpBits} Down: {bandwidthDownBits}",
    "tooltip-format-ethernet": "Û∞àÄ {ifname}\nIP: {ipaddr}\n up: {bandwidthUpBits} down: {bandwidthDownBits}",
    "tooltip-format-disconnected": "Disconnected",
    "max-length": 50,
    "on-click": "~/.config/.settings/networkmanager.sh"
  },
  "battery": {
    "states": {
      "warning": 30,
      "critical": 15
    },
    "format": "{icon}   {capacity}%",
    "format-charging": "Ó∫°   {capacity}%",
    "format-plugged": "Ôá¶  {capacity}%",
    "format-alt": "{icon}  {time}",
    "format-icons": [
      "ÔâÑ ",
      "ÔâÉ ",
      "ÔâÇ ",
      "ÔâÅ ",
      "ÔâÄ "
    ]
  },
  "pulseaudio": {
    "format": "{icon} {volume}%",
    "format-bluetooth": "{volume}% {icon}Ôäî {format_source}",
    "format-bluetooth-muted": "Û∞Ç≤  {icon}Ôäî {format_source}",
    "format-muted": "Û∞ùü {format_source}",
    "format-source": "{volume}% ÔÑ∞",
    "format-source-muted": "ÔÑ±",
    "format-icons": {
      "headphone": "ÔÄ•",
      "hands-free": "Û∞•é",
      "headset": "Óπô",
      "phone": "ÔÇï",
      "portable": "ÔÇï",
      "car": "ÔÜπ",
      "default": [
        "ÔÄ¶",
        "ÔÄ® ",
        "ÔÄ® "
      ]
    },
    "on-click": "pavucontrol"
  },
  "bluetooth": {
    "format": "Ôäì {status}",
    "format-disabled": "",
    "format-off": "",
    "interval": 30,
    "on-click": "blueman-manager",
    "format-no-controller": ""
  },
  "user": {
    "format": "{user}",
    "interval": 60,
    "icon": false
  },
  "idle_inhibitor": {
    "format": "{icon}",
    "tooltip": true,
    "format-icons": {
      "activated": "ÔÇú",
      "deactivated": "ÔÄ£"
    },
    "on-click-right": "hyprlock"
  }
}
